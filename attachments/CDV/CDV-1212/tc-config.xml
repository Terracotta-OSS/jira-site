  <servers>
    <server name="L2S2" host="localhost" bind="localhost">
      <l2-group-port>7201</l2-group-port>
      <dso-port>7202</dso-port>
      <jmx-port>7203</jmx-port>
      <data>/tmp/dfghwrthwdfgwggwrtterra/server2-data/%D</data>
      <logs>/tmp/dfghwrthwdfgwggwrtterra/server2-logs/%D</logs>
      <statistics>/tmp/dfghwrthwdfgwggwrtterra/cluster2-statistics/%D</statistics>
      <dso>
        <persistence>
          <mode>permanent-store</mode>
        </persistence>
        <client-reconnect-window>30</client-reconnect-window>
        <garbage-collection>
          <interval>300</interval>
        </garbage-collection>
      </dso>
    </server>
    <server name="L2S3" host="localhost" bind="localhost">
      <l2-group-port>7301</l2-group-port>
      <dso-port>7302</dso-port>
      <jmx-port>7303</jmx-port>
      <data>/tmp/dfghwrthwdfgwggwrtterra/server3-data/%D</data>
      <logs>/tmp/dfghwrthwdfgwggwrtterra/server3-logs/%D</logs>
      <statistics>/tmp/dfghwrthwdfgwggwrtterra/cluster3-statistics/%D</statistics>
      <dso>
        <persistence>
          <mode>permanent-store</mode>
        </persistence>
        <client-reconnect-window>30</client-reconnect-window>
        <garbage-collection>
          <interval>300</interval>
        </garbage-collection>
      </dso>
    </server>
    <server name="L2S1" host="localhost" bind="localhost">
      <l2-group-port>7101</l2-group-port>
      <dso-port>7102</dso-port>
      <jmx-port>7103</jmx-port>
      <data>/tmp/dfghwrthwdfgwggwrtterra/server1-data/%D</data>
      <logs>/tmp/dfghwrthwdfgwggwrtterra/server1-logs/%D</logs>
      <statistics>/tmp/dfghwrthwdfgwggwrtterra/cluster1-statistics/%D</statistics>
      <dso>
        <persistence>
          <mode>permanent-store</mode>
        </persistence>
        <client-reconnect-window>30</client-reconnect-window>
        <garbage-collection>
          <interval>300</interval>
        </garbage-collection>
      </dso>
    </server>
    <ha>
      <mode>networked-active-passive</mode>
      <networked-active-passive>
        <election-time>5</election-time>
      </networked-active-passive>
    </ha>
    <update-check>
      <enabled>false</enabled>
      <period-days>10</period-days>
    </update-check>
  </servers>
  <clients>
  <!--
    <modules>
      <module name="tim-jmx-util" version="1.0.3" />
    </modules>
    -->
    <logs>/tmp/dfghwrthwdfgwggwrtterra/client-logs/%D</logs>
    <statistics>/tmp/dfghwrthwdfgwggwrtterra/client-statistics/%D</statistics>
    <dso>
      <debugging>
        <instrumentation-logging>
          <roots>true</roots>
          <distributed-methods>true</distributed-methods>
          <locks>true</locks>
          <class>true</class>
        </instrumentation-logging>
        <runtime-logging>
          <lock-debug>true</lock-debug>
          <distributed-method-debug>true</distributed-method-debug>
          <field-change-debug>true</field-change-debug>
          <wait-notify-debug>true</wait-notify-debug>
          <new-object-debug>true</new-object-debug>
        </runtime-logging>
      </debugging>
    </dso>
  </clients>
  <application>
    <dso>
      <locks>
        <autolock auto-synchronized="false">
          <method-expression>int org.zzz.eto.Dat.getVal()</method-expression>
          <lock-level>synchronous-write</lock-level>
        </autolock>
        <autolock auto-synchronized="false">
          <method-expression>org.zzz.eto.Dat org.zzz.eto.Dat.getInstance()</method-expression>
          <lock-level>read</lock-level>
        </autolock>
      </locks>
      <instrumented-classes>
        <include>
          <class-expression>org.zzz.eto.Dat</class-expression>
        </include>
      </instrumented-classes>
      <roots>
        <root>
          <field-name>org.zzz.eto.Dat.instance</field-name>
        </root>
        <root>
          <field-name>org.zzz.eto.Dat.lock</field-name>
        </root>
      </roots>
    </dso>
  </application>
</con:tc-config>